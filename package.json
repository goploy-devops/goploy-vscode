{
  "name": "goploy",
  "displayName": "goploy",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:namespace"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Your Extension",
      "properties": {
        "goploy.apiKey": {
          "type": "string",
          "default": "",
          "description": "goploy api key"
        },
        "goploy.domain": {
          "type": "string",
          "default": "",
          "description": "goploy domain like https://example.com"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "goploy",
          "title": "Goploy",
          "icon": "./src/logo/logo.svg"
        }
      ]
    },
    "views": {
      "goploy": [
        {
          "id": "namespace",
          "name": "Namespace"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.openSettings",
        "title": "Extension Settings"
      },
      {
        "command": "namespace.refreshEntry",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "namespace.runEntry",
        "title": "Run",
        "icon": "$(run)"
      },
      {
        "command": "namespace.branchEntry",
        "title": "Branch",
        "icon": "$(git-branch)"
      },
      {
        "command": "namespace.tagEntry",
        "title": "Tag",
        "icon": "$(tag)"
      },
      {
        "command": "namespace.rebuildEntry",
        "title": "Revert to",
        "icon": "$(notebook-revert)"
      },
      {
        "command": "namespace.resultEntry",
        "title": "Result",
        "icon": "$(note)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "namespace.refreshEntry",
          "when": "view == namespace",
          "group": "navigation"
        },
        {
          "command": "extension.openSettings"
        }
      ],
      "view/item/context": [
        {
          "command": "namespace.runEntry",
          "when": "view == namespace && viewItem == namespaceProjectItem",
          "group": "inline@1"
        },
        {
          "command": "namespace.branchEntry",
          "when": "view == namespace && viewItem == namespaceProjectItem",
          "group": "inline@2"
        },
        {
          "command": "namespace.tagEntry",
          "when": "view == namespace && viewItem == namespaceProjectItem",
          "group": "inline@3"
        },
        {
          "command": "namespace.rebuildEntry",
          "when": "view == namespace && viewItem == namespaceProjectItem",
          "group": "inline@4"
        },
        {
          "command": "namespace.resultEntry",
          "when": "view == namespace && viewItem == namespaceProjectItem",
          "group": "inline@5"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/node-fetch": "^2.5.12",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "axios": "^0.26.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  }
}
